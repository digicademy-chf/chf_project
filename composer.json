        "typo3/cms-scheduler": "^13",
        "typo3/cms-seo": "^13",
        "typo3/cms-setup": "^13",
        "typo3/cms-tstemplate": "^13",
        "typo3/cms-viewpage": "^13",
        "typo3/cms-webhooks": "^13",
        "typo3/cms-workspaces": "^13",
        "typo3/minimal": "^13"
