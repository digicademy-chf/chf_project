{
    "name": "digicademy/chf_project",
    "type": "typo3-cms-extension",
    "description": "Boilerplate TYPO3 sitepackage for the Cultural Heritage Framework",
    "authors": [
        {
            "name": "Jonatan Jalle Steller",
            "email": "jonatan.steller@adwmainz.de",
            "role": "Developer",
            "homepage": "https://www.adwmainz.de"
        },
        {
            "name": "CHF Project contributors"
        }
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://extensions.typo3.org/extension/chf_project",
    "support": {
        "issues": "https://github.com/digicademy-chf/chf_project/issues",
        "source": "https://github.com/digicademy-chf/chf_project",
        "docs": "https://digicademy-chf.github.io/chf_project"
    },
    "keywords": [
        "TYPO3",
        "Sitepackage",
        "Boilerplate",
        "Accessibility",
        "Web app",
        "Data",
        "Research",
        "Academia",
        "Academic"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_base.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_bib.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_lex.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_map.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_gloss.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_pub.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_object.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy-chf/chf_media.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy/vis.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/digicademy/qad.git"
        }
    ],
    "require": {
        "typo3/cms-adminpanel": "^12",
        "typo3/cms-backend": "^12",
        "typo3/cms-belog": "^12",
        "typo3/cms-beuser": "^12",
        "typo3/cms-core": "^12",
        "typo3/cms-dashboard": "^12",
        "typo3/cms-extbase": "^12",
        "typo3/cms-extensionmanager": "^12",
        "typo3/cms-felogin": "^12",
        "typo3/cms-filelist": "^12",
        "typo3/cms-filemetadata": "^12",
        "typo3/cms-fluid": "^12",
        "typo3/cms-fluid-styled-content": "^12",
        "typo3/cms-form": "^12",
        "typo3/cms-frontend": "^12",
        "typo3/cms-impexp": "^12",
        "typo3/cms-info": "^12",
        "typo3/cms-install": "^12",
        "typo3/cms-linkvalidator": "^12",
        "typo3/cms-lowlevel": "^12",
        "typo3/cms-opendocs": "^12",
        "typo3/cms-reactions": "^12",
        "typo3/cms-recycler": "^12",
        "typo3/cms-redirects": "^12",
        "typo3/cms-reports": "^12",
        "typo3/cms-rte-ckeditor": "^12",
        "typo3/cms-scheduler": "^12",
        "typo3/cms-seo": "^12",
        "typo3/cms-setup": "^12",
        "typo3/cms-sys-note": "^12",
        "typo3/cms-t3editor": "^12",
        "typo3/cms-tstemplate": "^12",
        "typo3/cms-viewpage": "^12",
        "typo3/cms-webhooks": "^12",
        "typo3/cms-workspaces": "^12",
        "digicademy/chf_base": "dev-main",
        "digicademy/chf_bib": "dev-main",
        "digicademy/chf_lex": "dev-main",
        "digicademy/chf_map": "dev-main",
        "digicademy/chf_gloss": "dev-main",
        "digicademy/chf_pub": "dev-main",
        "digicademy/chf_object": "dev-main",
        "digicademy/chf_media": "dev-main",
        "digicademy/vis": "dev-main",
        "digicademy/qad": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Digicademy\\CHFProject\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "chf_project"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
