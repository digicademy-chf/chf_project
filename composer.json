        "typo3/cms-belog": "^13",
        "typo3/cms-beuser": "^13",
        "typo3/cms-core": "^13",
        "typo3/cms-dashboard": "^13",
        "typo3/cms-extbase": "^13",
        "typo3/cms-extensionmanager": "^13",
        "typo3/cms-filelist": "^13",
