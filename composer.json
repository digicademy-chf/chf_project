    "autoload": {
        "psr-4": {
            "Digicademy\\CHFProject\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "chf_project"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
